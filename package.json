{
  "name": "llama-airforce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "lint laf": "eslint src/Apps/LlamaAirforce/**/*.{ts,vue}",
    "lint cm": "eslint src/Apps/CurveMonitor/**/*.{ts,vue}",
    "dev laf": "vite src/Apps/LlamaAirforce",
    "dev cm": "vite src/Apps/CurveMonitor",
    "build laf": "eslint src/Apps/LlamaAirforce/**/*.{ts,vue} && vue-tsc --noEmit && vite build src/Apps/LlamaAirforce",
    "build cm": "eslint src/Apps/CurveMonitor/**/*.{ts,vue} && vue-tsc --noEmit && vite build src/Apps/CurveMonitor",
    "build contracts": "typechain --target ethers-v5 --input-dir \"./src/ABI\" --out-dir \"./src/Contracts\" \"./src/ABI/**/*.json\"",
    "test": "vitest --silent",
    "union": "node --no-lazy -r ts-node/register -r tsconfig-paths/register \"./src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.ts\"",
    "union migrate": "node --no-lazy -r ts-node/register -r tsconfig-paths/register \"./src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.migrate.ts\""
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@kyvg/vue3-notification": "^2.9.1",
    "@web3-onboard/coinbase": "^2.2.4",
    "@web3-onboard/core": "^2.19.2",
    "@web3-onboard/gnosis": "^2.1.10",
    "@web3-onboard/injected-wallets": "^2.9.0",
    "@web3-onboard/walletconnect": "^2.3.8",
    "apexcharts": "^3.40.0",
    "class-transformer": "^0.5.1",
    "cross-fetch": "^3.1.6",
    "ethers": "^5.7.2",
    "lightweight-charts": "^4.0.1",
    "lodash": "^4.17.21",
    "pinia": "^2.1.3",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.6.2",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2",
    "vue3-apexcharts": "^1.4.1",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@azure/cosmos": "^3.17.3",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@gnosis.pm/safe-apps-sdk": "^7.8.0",
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@safe-global/safe-core-sdk": "^3.3.3",
    "@safe-global/safe-core-sdk-types": "^1.10.1",
    "@safe-global/safe-core-sdk-utils": "^1.7.3",
    "@safe-global/safe-ethers-lib": "^1.9.3",
    "@safe-global/safe-service-client": "^2.0.1",
    "@typechain/ethers-v5": "^11.0.0",
    "@types/lodash": "^4.14.195",
    "@types/readline-sync": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "barrelsby": "^2.8.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.1.3",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.14.1",
    "ethereum-multicall": "^2.17.0",
